<!DOCTYPE html>
<html>
<head>
  <title><%= @title || "Timarche" %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
  <%= render :partial => 'layouts/googlefont'%>
  <%= analytics_init if Rails.env.production? %>
  <%= render :partial => 'layouts/sharethis'%> 
  <%= render :partial => 'layouts/sumome'%>
  <%= javascript_include_tag "//www.google.com/jsapi", "chartkick" %> 
  <%if Rails.env.production?%>
  <%= render :partial => 'layouts/zopim'%>
  <%end%>
</head>
	  <body>
    
    <% if content_for?(:body_attributes) %>
      <%= yield(:body_attributes) %> 
    <% end %>
		<% unless @title == "welcome" %>
		   <%= render :partial => 'shared/header'%>
		<%end%>
		<div class="container">
	       <%= flash_messages %>
		</div>
    <div class="modal" id="spinner" style="display:none;"></div>
		<%= yield :koudoku %>
		<%= yield %>
		<%= render :partial => 'shared/footer'%>
    </body>
</html>
