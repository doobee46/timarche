<!DOCTYPE html>
<html>
<head>
  <title><%= @title || "Timarche" %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
  <%= analytics_init if Rails.env.production? %>
  <%= render :partial => 'layouts/googlefonts'%>
  <%= render :partial => 'layouts/banner'%>
  <%if Rails.env.production?%>
  <%= render :partial => 'layouts/zopim'%>
  <%= render :partial => 'layouts/getonboarded'%>
  <%= render :partial => 'layouts/satismeter'%>
  <%end%>
</head>
	<body>
        <% if content_for?(:body_attributes) %>
          <%= yield(:body_attributes) %> 
        <% end %>>
		<% unless @title == "welcome" %>
		   <%= render :partial => 'shared/header'%>
		<%end%>
		<%= render :partial => 'layouts/addthis'%>	
		<div class="container">
	       <%= flash_messages %>
		</div>
		<%= yield :koudoku %>
		<%= yield %>
		<%= render :partial => 'shared/footer'%>
	</body>
</html>
