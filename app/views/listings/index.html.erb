<body id="bd_list">
<% @title = 'browse' %>
<div class="container clearfix">
  <div class="row">
    <div class="col-md-12">
      <div class="main_pages switcher">
        <h3>Touts les Annonces</h3>
        <p>Découvrez les annonces de partout dans le marché.</p>
        <div class="btn-group" role="group" aria-label="...">
          <%= link_to recent_path ,class: "btn btn-default" do %>
            <span class="fa fa-clock-o"></span>Recent
          <%end%>
          <%= link_to popular_path , class: "btn btn-default" do %>
            <span class="fa fa-fire"></span>Trending
          <%end%>
        </div>
      </div>     
     <div class="item_list" >
        <div id="listings">
          <%= render @listings %>
       </div>        
     </div>
       <%= will_paginate @listings  %>
  </div>
</div>

